{
    "name": "tzappa/phpiko",
    "description": "Small PHP codebase for a web application",
    "type": "project",
    "license": "Unlicense",
    "require": {
        "php": ">=8.2",
        "psr/cache": "^3.0",
        "psr/container": "^2.0",
        "psr/event-dispatcher": "^1.0",
        "psr/log": "^3.0",
        "psr/http-message": "^2.0",
        "psr/http-server-handler": "^1.0",
        "psr/http-server-middleware": "^1.0",
        "laminas/laminas-diactoros": "^3.6",
        "laminas/laminas-httphandlerrunner": "^2.12",
        "twig/twig": "^3.21",
        "bjeavons/zxcvbn-php": "^1.4"
    },
    "require-dev": {
        "symfony/var-dumper": "^7.3",
        "phpunit/phpunit": "^11.5",
        "pds/skeleton": "^1.1",
        "phpstan/phpstan": "^2.1",
        "squizlabs/php_codesniffer": "^3.13"
    },
    "autoload": {
        "psr-4": {
            "Admin\\": "src/Admin/",
            "API\\": "src/API/",
            "App\\": "src/App/",
            "Clear\\": "src/Clear/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": [
            "phpunit"
        ],
        "lint": "phpcs --standard=PSR12 src tests",
        "lint-fix": "phpcbf --standard=PSR12 src tests"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
